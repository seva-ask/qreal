<local:Arrow x:Class="ObjectTypes.Arrows.CompositionArrow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:ObjectTypes"
    mc:Ignorable="d"
    MaxHeight="10" MinHeight="10" MaxWidth="10" MinWidth="10">
    <Grid x:Name="LayoutRoot">
    	<Grid.Background>
    		<ImageBrush Stretch="Fill"/>
    	</Grid.Background>
    	<Rectangle Fill="Black" Stroke="Black" RenderTransformOrigin="0.5,0.5" Width="6" Height="6" HorizontalAlignment="Center" VerticalAlignment="Center">
    		<Rectangle.RenderTransform>
    			<CompositeTransform Rotation="45"/>
    		</Rectangle.RenderTransform>
    	</Rectangle>
    </Grid>
</local:Arrow>