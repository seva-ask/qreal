<base:NodeType
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:base="clr-namespace:ObjectTypes;assembly=ObjectTypes"
    x:Name="nodeType" x:Class="ClassDiagram.ClassFieldType"
    mc:Ignorable="d"
    Width="150" Height="20" Background="Transparent" Foreground="Transparent" MinHeight="20">
    <Grid x:Name="LayoutRoot" Background="Transparent">
		<TextBlock TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Text="{Binding DataContext.LogicalInstance.Name, ElementName=nodeType}" Foreground="Black"/>
	</Grid>
</base:NodeType>
