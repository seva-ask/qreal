<local:Arrow
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:ObjectTypes"
    xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing" x:Class="ObjectTypes.Arrows.GeneralizationArrow"
    mc:Ignorable="d"
    MaxHeight="10" MinHeight="10" MaxWidth="10" MinWidth="10">
	<Canvas x:Name="LayoutRoot" Background="Transparent">
		<ed:RegularPolygon Fill="Black" Height="10" Stretch="Fill" PointCount="3" Stroke="Black" Canvas.Top="5" UseLayoutRounding="False" Width="10">
			<ed:RegularPolygon.RenderTransform>
				<CompositeTransform Rotation="270"/>
			</ed:RegularPolygon.RenderTransform>
		</ed:RegularPolygon>
	</Canvas>
</local:Arrow>
